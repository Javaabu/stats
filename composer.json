{
    "name": "javaabu/stats",
    "description": "Simplifies stats generation for Laravel",
    "homepage": "https://github.com/Javaabu/stats",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "stats"
    ],
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "maatwebsite/excel": "^3.1",
        "javaabu/forms": "^1.22",
        "javaabu/settings": "^1.5"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.2",
        "phpunit/phpunit": "^9.5 || ^10.5",
        "spatie/laravel-activitylog": "^4.8",
        "plannr/laravel-fast-refresh-database": "^1.2"
    },
    "suggest": {
        "spatie/laravel-activitylog": "Required for included activity log based stats"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Stats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Stats\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Stats\\StatsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
